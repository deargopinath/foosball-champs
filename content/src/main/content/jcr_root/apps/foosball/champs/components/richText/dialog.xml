<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    height="480"
    width="640"
    title="Rich Text - Dialog"
    xtype="dialog">
    <items jcr:primaryType="cq:TabPanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Settings">
                <items jcr:primaryType="cq:WidgetCollection">
                    <richTextItem
                            jcr:primaryType="cq:Widget"
                            enableStyle="{Boolean}false"
                            hideLabel="{Boolean}true"
                            height="460"
                            width="600"
                            name="./text"
                            xtype="richtext">
                        <rtePlugins jcr:primaryType="nt:unstructured">
                            <edit
                                    jcr:primaryType="nt:unstructured"
                                    features="*"/>
                            <format
                                    jcr:primaryType="nt:unstructured"
                                    features="[bold,italic]"/>
                            <image
                                    jcr:primaryType="nt:unstructured"
                                    features="*"/>
                            <justify
                                    jcr:primaryType="nt:unstructured"
                                    features="*"/>
                            <lists
                                    jcr:primaryType="nt:unstructured"
                                    features="*"/>
                            <misctools
                                    jcr:primaryType="nt:unstructured"
                                    features="*">
                                <specialCharsConfig jcr:primaryType="nt:unstructured">
                                    <chars jcr:primaryType="nt:unstructured">
                                        <Mu
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;Mu;"/>
                                        <Omega
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;Omega;"/>
                                        <Omicron
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;Omicron;"/>
                                        <beta
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;beta;"/>
                                        <copyright
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;copy;"/>
                                        <degree
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;deg;"/>
                                        <division
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;divide;"/>
                                        <em-dash
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;mdash;"/>
                                        <en-dash
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;ndash;"/>
                                        <left-double-quote
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;ldquo;"/>
                                        <left-single-quote
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;lsquo;"/>
                                        <lower-a-acute
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;aacute;"/>
                                        <lower-a-grave
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;agrave;"/>
                                        <lower-a-tilde
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;atilde;"/>
                                        <lower-a-umlaut
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;auml;"/>
                                        <lower-c-cedilla
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;ccedil;"/>
                                        <lower-e-acute
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;eacute;"/>
                                        <lower-e-grave
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;egrave;"/>
                                        <lower-e-umlaut
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;euml;"/>
                                        <lower-i-umlaut
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;iuml;"/>
                                        <lower-o-umlaut
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;ouml;"/>
                                        <lower-sharps
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;szlig;"/>
                                        <lower-u-accute
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;uacute;"/>
                                        <lower-u-grave
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;ugrave;"/>
                                        <lower-u-umlaut
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;uuml;"/>
                                        <macron
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;macr;"/>
                                        <micro-sign
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;micro;"/>
                                        <mu
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;mu;"/>
                                        <omega
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;omega;"/>
                                        <omicron
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;omicron;"/>
                                        <one-fourth
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;frac14;"/>
                                        <one-half
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;frac12;"/>
                                        <plus-or-minus
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;plusmn;"/>
                                        <registered-trademark
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;reg;"/>
                                        <right-double-quote
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;rdquo;"/>
                                        <right-single-quote
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;rsquo;"/>
                                        <single-low-9-quote
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;sbquo;"/>
                                        <superscript-1
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;sup1;"/>
                                        <superscript-2
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;sup2;"/>
                                        <superscript-3
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;sup3;"/>
                                        <three-fourths
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;frac34;"/>
                                        <trademark
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;trade;"/>
                                        <upper-a-accute
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;Aacute;"/>
                                        <upper-a-circumflex
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;Acirc;"/>
                                        <upper-a-grave
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;Agrave;"/>
                                        <upper-a-ring
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;Aring;"/>
                                        <upper-a-tilde
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;Atilde;"/>
                                        <upper-a-umlaut
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;Auml;"/>
                                        <upper-c-cedilla
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;Ccedil;"/>
                                        <upper-e-acute
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;Eacute;"/>
                                        <upper-e-grave
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;Egrave;"/>
                                        <upper-o-umlaut
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;Ouml;"/>
                                        <upper-u-acute
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;Uacute;"/>
                                        <upper-u-grave
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;Ugrave;"/>
                                        <upper-u-umlaut
                                                jcr:primaryType="nt:unstructured"
                                                entity="&amp;Uuml;"/>
                                    </chars>
                                </specialCharsConfig>
                            </misctools>
                            <paraformat
                                    jcr:primaryType="nt:unstructured"
                                    features="*"/>
                            <undo
                                    jcr:primaryType="nt:unstructured"
                                    features="*"/>
                        </rtePlugins>
                    </richTextItem>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>